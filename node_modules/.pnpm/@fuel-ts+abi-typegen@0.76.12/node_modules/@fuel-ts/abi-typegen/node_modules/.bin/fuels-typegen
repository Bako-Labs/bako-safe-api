#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/@fuel-ts+abi-typegen@0.76.12/node_modules/@fuel-ts/abi-typegen/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/@fuel-ts+abi-typegen@0.76.12/node_modules/@fuel-ts/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/@fuel-ts+abi-typegen@0.76.12/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/@fuel-ts+abi-typegen@0.76.12/node_modules/@fuel-ts/abi-typegen/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/@fuel-ts+abi-typegen@0.76.12/node_modules/@fuel-ts/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/@fuel-ts+abi-typegen@0.76.12/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../typegen.js" "$@"
else
  exec node  "$basedir/../../typegen.js" "$@"
fi
